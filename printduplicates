#include <bits/stdc++.h>
using namespace std;

void printduplicates(string str){
    map<char,int> m;
    for(int i=0;i<str.length();i++){
        m[str[i]]++;
    }
    for(auto it:m){
        if(it.second>1)
        cout<<it.first<<"-"<<it.second<<"\n";
        
    }
}
int main(){
    string str="abaaachfkshihdslihdlhwsldsa";
    printduplicates(str);
    return 0;
}
